 */
this.fn_PostCreateFile = function()
{
	var url = this.dsFileUrl.getColumn(0, "fileUrl");
	this.FileDownload.set_downloadurl(url);
	this.FileDownload.download();
	this.gfn_getRowCount(this.staRowCnt,this.dsMaster);
};

