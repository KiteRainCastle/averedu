					var strCallBack = "fn_callBack";    //공백일시 기본 fn_callBack
					var strASync    = false;             //생략이나 공백일시에는 ASync=true,싱크시는 false로
					this.gfn_Transaction(strSvc
									   , strUrl
									   , strInDs
									   , strOutDs
									   , strArg
									   , strCallBack
