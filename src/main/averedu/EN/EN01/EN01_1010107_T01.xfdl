};
/**********************************************************************
        04. 공통 코드 및 콤보 데이타 조회
***********************************************************************/
this.fn_formInit = function()
{
    var strDs    = "dsSearchJeokyong|dsGridJeokyong|dsSearchGwamok|dsGridGwamok|dsGwamok|dsSearchYn|dsGridYn|dsYn";                    // 데이터를 받을 데이터셋리스트     예) "DS_CODE01|DS_CODE02|DS_CODE03"
