 **/
this.fn_beforeclose = function()
{
   var value = false;
   
   if(this.gfn_isUpdate(this.dsMaster))
   {
      value = true;
   }
   else if(this.gfn_isUpdate(this.dsDetail))
   {
      value = true;
   }

   return value;
};

