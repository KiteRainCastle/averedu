	var row = this.dsGijun.rowposition;
	this.ds_habbanInput.setColumn(0, "YYYY", this.dsGijun.getColumn(row, "YYYY"));
	this.ds_habbanInput.setColumn(0, "HAKGI", this.dsGijun.getColumn(row, "HAKGI"));
	this.ds_habbanInput.setColumn(0, "HAKGWA_CD", this.dsGijun.getColumn(row, "HAKGWA_CD"));
	this.ds_habbanInput.setColumn(0, "JUYA_GBCD", this.dsGijun.getColumn(row, "JUYA_GBCD"));
	this.ds_habbanInput.setColumn(0, "ISU_HAKNYEON_CD", this.dsGijun.getColumn(row, "ISU_HAKNYEON_CD"));
	this.ds_habbanInput.setColumn(0, "GYOGWAMOK_CD", this.dsGijun.getColumn(row, "GYOGWAMOK_CD"));
	this.ds_habbanInput.setColumn(0, "BUNBAN", this.dsGijun.getColumn(row, "BUNBAN"));
