	this.dsHabban.copyRow(newrowposition, this.dsMaster, rowposition);
	
	if (this.dsHaksaengCopy.rowcount > 0)
	{
		if (this.dsHaksaeng.rowcount == 0)
		{
			this.dsHaksaeng.copyData(this.dsHaksaengCopy);
		}
		else
		{
			for(var i = 0 ; i < this.dsHaksaengCopy.rowcount ; i++)
			{
				var newRow = this.dsHaksaeng.addRow();
				this.dsHaksaeng.copyRow(newRow, this.dsHaksaengCopy, i);
			}
		}
	}	
	
	this.gfn_getRowCount(this.staRowCnt2,this.dsHaksaeng);
};

