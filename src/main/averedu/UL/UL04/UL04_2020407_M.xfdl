// 합반처리과목 추가
this.btnDown2_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var rowposition = this.dsMaster.rowposition;
	var bunban = this.dsMaster.getColumn(this.dsMaster.rowposition,'BUNBAN');
	var gyogwamok_cd = this.dsMaster.getColumn(this.dsMaster.rowposition,'GYOGWAMOK_CD');
	
	//기준과목의 동일과목 체크
	var gijun_dongilGuamokBunbanChk = this.dsGijun.findRowExpr("GYOGWAMOK_CD == '"+gyogwamok_cd+"' && BUNBAN == '"+bunban+"'");
	var gijun_dongilGuamokChk = this.dsGijun.findRowExpr("GYOGWAMOK_CD == '"+gyogwamok_cd+"'");
	
	//합반처리과목 동일과목 체크
	var hapban_gijun_dongilGuamokBunbanChk = this.dsHabban.findRowExpr("GYOGWAMOK_CD == '"+gyogwamok_cd+"' && BUNBAN == '"+bunban+"'");
	
	//분반과 과목코드가 같을 경우
	if (gijun_dongilGuamokBunbanChk > -1)
	{
		this.alert("기준과목과 동일한 과목은 추가할 수 없습니다.");
		return false;
	}
	
	//동일과목 다른 분반일 경우
	if (gijun_dongilGuamokChk > -1)
	{
		//합분반 데이터셋 addRow
		if (rowposition!= -1 && hapban_gijun_dongilGuamokBunbanChk == -1)
		{
			this.fn_HaksaengRet();
		}
		else
		{
			this.alert("동일한 분반을 추가하실 필요가 없습니다.");
			return false;
		}
	}
	else
	{
		this.alert("동일교과목의 다른분반만 합반이 가능합니다.");
		return false;
	}
};

// 합반처리과목 삭제
this.btnUp2_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var rowposition = this.dsHabban.rowposition;
	
	if (this.dsHabban.getRowType(rowposition) == 1)
	{
		this.alert("합반 처리 된 과목은 취소가 불가능합니다.");
		return false;
	}
	else
	{	
		var bunban = this.dsHabban.getColumn(this.dsHabban.rowposition,'BUNBAN');
		var sugangsincheong_cnt = this.dsHabban.getColumn(this.dsHabban.rowposition, 'SUGANGSINCHEONG_CNT');
		
		this.dsHabban.deleteRow(rowposition);			
		this.dsHaksaeng.set_enableevent(false);
		
		var row = this.dsHaksaeng.findRowExpr("BUNBAN =='"+bunban+"'");
		var delArray = new Array();
		
		for(var i = row ; i <= row+sugangsincheong_cnt-1 ; i++)
		{
			delArray.push(i);
		}
		
		this.dsHaksaeng.deleteMultiRows(delArray);
		this.dsHaksaeng.set_enableevent(true);			
	}
};

