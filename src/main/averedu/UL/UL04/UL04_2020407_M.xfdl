this.fn_PreSave = function()
{
	var result = this.gfn_confirm( "합반처리 하시겠습니까?", "저장","", "question" );
	if(result == 0)
	{
		return false;
	}
	 
	this.dsSave.clearData();
	
	for(var i = 0 ; i < this.dsHabban.rowcount ; i++)
	{
		this.dsSave.addRow();
		this.dsSave.setColumn(i,0,this.dsHabban.getColumn(i,'YYYY'));
		this.dsSave.setColumn(i,1,this.dsHabban.getColumn(i,'HAKGI'));
		this.dsSave.setColumn(i,2,this.dsHabban.getColumn(i,'HAKGWA_CD'));
		this.dsSave.setColumn(i,3,this.dsHabban.getColumn(i,'JUYA_GBCD'));
		this.dsSave.setColumn(i,4,this.dsHabban.getColumn(i,'ISU_HAKNYEON_CD'));
		this.dsSave.setColumn(i,5,this.dsHabban.getColumn(i,'GYOGWAMOK_CD'));
		this.dsSave.setColumn(i,6,this.dsHabban.getColumn(i,'BUNBAN'));
		
		this.dsSave.setColumn(i,7,this.dsGijun.getColumn(this.dsGijun.rowposition,'YYYY'));
		this.dsSave.setColumn(i,8,this.dsGijun.getColumn(this.dsGijun.rowposition,'HAKGI'));
		this.dsSave.setColumn(i,9,this.dsGijun.getColumn(this.dsGijun.rowposition,'HAKGWA_CD'));
		this.dsSave.setColumn(i,10,this.dsGijun.getColumn(this.dsGijun.rowposition,'JUYA_GBCD'));
		this.dsSave.setColumn(i,11,this.dsGijun.getColumn(this.dsGijun.rowposition,'ISU_HAKNYEON_CD'));
		this.dsSave.setColumn(i,12,this.dsGijun.getColumn(this.dsGijun.rowposition,'GYOGWAMOK_CD'));
		this.dsSave.setColumn(i,13,this.dsGijun.getColumn(this.dsGijun.rowposition,'BUNBAN'));
	}	

	return true;
};

