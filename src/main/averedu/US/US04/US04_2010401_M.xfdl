			var result = this.gfn_confirm(jeungmyeongseoNm + "를 출력하시겠습니까?", "저장","", "question" );
			if(result == 0)
			{
				return false;
			}
			
			this.dsTemp.clearData();
			var nRow = this.dsTemp.addRow();
			this.dsTemp.copyRow(nRow, this.dsMaster, e.row);
			this.fn_Print();
		}	
	}
	
	// 재출력
	if (e.col == 14) 
	{
		this.fn_RePrint(e.row);
	}	
};

