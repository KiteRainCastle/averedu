    var result = this.gfn_confirm( "현재선택행을 삭제하시겠습니까?", "삭제정보","","question" );

    if(result == 0)
    {
        return false;
    }

    //다중삭제 용도
    this.dsBook.set_enableevent(false);
    for(var i=this.dsBook.rowcount-1;i>-1;i--)
