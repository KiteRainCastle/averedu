	{
		this.alert("생성년도를 입력해주세요.");
		return false;
	}
	
	if(gijunYyyy > saengsungYyyy)
	{
		this.alert("기준년도는 생성년도보다 클 수 없습니다.");
		return false;
	}
	
	var result = this.gfn_confirm( "복사 하시겠습니까?", "저장","", "question" );
