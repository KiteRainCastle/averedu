						this.gfn_Transaction(strSvc
										   , strUrl
										   , strInDs
										   , strOutDs
										   , strArg
										   , strCallBack
										   , strASync);					
					}

				} else if(sRtn[0] == "M")
				{
				} else if(sRtn[0] == "CLOSE")
				{
					
				} else if(sRtn[0] == "EXIT")
