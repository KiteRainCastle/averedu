	else if(HAKGI == '21')
	{
		this.gfn_commonUtils_report('UR03/UR03_2050304_M_02.crf',  param1);
	}
	else
	{
		this.gfn_commonUtils_report('UR03/UR03_2050304_M_03.crf',  param2);
	}
};

