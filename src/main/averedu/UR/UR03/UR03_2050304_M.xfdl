this.fn_formInit = function()
{
    var strDs    = "dsHakgi|dsJuya|dsHaknyeon|dsHakjeok|dsNapbu|dsBank";                    // 데이터를 받을 데이터셋리스트     예) "DS_CODE01|DS_CODE02|DS_CODE03"
