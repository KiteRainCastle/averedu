    {
        return false;
    }
    var strSvc      = "Ret";
    var strUrl      = "/prj/UR/UR03/Retrieve_2050305_M.do";
    var strInDs     = "ds_input=ds_input";
    var strOutDs    = "dsDaesangja=dsDaesangja ";
