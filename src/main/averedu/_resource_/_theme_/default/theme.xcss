<?xml version="1.0" encoding="utf-8"?>
<XCSS version="1.0"><![CDATA[
/*****************************************************************************************/
/******************** NEXACOR 17 기본사항이니 절대 지우지마세요. ********************/
/***************************************************************************************/

/******************** Frame/Form ********************/
.Form
{
	-nexa-border 			: 0px none;
	font 				: 12px "Malgun Gothic";
	color 				: #363636;
	background 			: #ffffff;
}
.Form[userstatus=contents]
{
	background 			: transparent;
}
.Form[status=disabled]
{
	color 				: #999999;
}

/******************** 테두리 지정 ********************/
.MainFrame,
.ChildFrame,
.FrameSet,
.VFrameSet,
.HFrameSet,
.TileFrameSet
{
	-nexa-border 			: 0px solid #00293e;
}
.MainFrame .titlebar .titleicon
{
	/*-nexa-icon 			: URL('theme://images/titlebar_icon_nexacro17.ico');*/
}


/********************** Button **********************/
.Button
{
	-nexa-border 			: 1px solid #c6c6c6;
	background 			: #f0f0f0;
	border-radius 			: 3px;
	color 				: #363636;
	font 				: bold 12px "Malgun Gothic";
}


.Button.test
{
	-nexa-border 			: 1px solid #c6c6c6;
	background 			: blue;
	border-radius 			: 3px;
	color 				: #363636;
	font 				: bold 12px "Malgun Gothic";
}.Button[status=focused],.Button[status=mouseover]
{
	-nexa-border 			: 1px solid #a0a0a0;
	background 			: #c6c6c6;
}
.Button[userstatus=pushed],.Button[userstatus=selected]
{
	background 			: #e9e9e9;
}
.Button[status=disabled]
{
	-nexa-border 			: 1px solid #d9d9d9;
	background 			: #f5f5f5;
	color 				: #bbbbbb;
	opacity 			: 0.5;
}
.ButtonControl
{
	-nexa-border 			: 0px none;
	background 			: transparent;
}

/********************** Calendar **********************/
.Calendar,.CalendarControl
{
	-nexa-border 			: 1px solid #c6c6c6;
	background 			: #ffffff;
	color 				: #363636;
}
.Calendar[status=focused],.CalendarControl[status=focused],
.Calendar[status=mouseover],.CalendarControl[status=mouseover]
{
	-nexa-border 			: 1px solid #3da2df;
}
.Calendar[status=readonly],.CalendarControl[status=readonly]
{
	background 			: #efefef;
	color 				: #363636;
}
.Calendar[status=disabled],.CalendarControl[status=disabled]
{
	background 			: #efefef;
	color 				: #999999;
}

.Calendar .calendaredit,
.CalendarControl .calendaredit
{
	/*MaskEditControl과 동일*/
}

.Calendar .dropbutton,
.CalendarControl .dropbutton
{
	-nexa-icon 			: URL('theme://images/btn_WF_Calendar.png');
}

.Calendar .dropbutton[status=disabled],
.CalendarControl .dropbutton[status=disabled]
{
	-nexa-icon 			: URL('theme://images/btn_WF_Calendar_D.png');
}

.Calendar .calendarspinupbutton,
.CalendarControl .calendarspinupbutton
{
	-nexa-icon 			: URL('theme://images/btn_WF_Spinup.png');
}

.Calendar .calendarspinupbutton[status=mouseover],
.CalendarControl .calendarspinupbutton[status=mouseover]
{
	-nexa-icon 			: URL('theme://images/btn_WF_Spinup_O.png');
}

.Calendar .calendarspinupbutton[userstatus=pushed],
.CalendarControl .calendarspinupbutton[userstatus=pushed]
{
	-nexa-icon 			: URL('theme://images/btn_WF_Spinup_P.png');
}

.Calendar .calendarspinupbutton[status=disabled],
.CalendarControl .calendarspinupbutton[status=disabled]
{
	-nexa-icon 			: URL('theme://images/btn_WF_Spinup_D.png');
}

.Calendar .calendarspindownbutton,.CalendarControl .calendarspindownbutton
{
	-nexa-icon 			: URL('theme://images/btn_WF_Spindown.png');
}

.Calendar .calendarspindownbutton[status=mouseover],
.CalendarControl .calendarspindownbutton[status=mouseover]
{
	-nexa-icon 			: URL('theme://images/btn_WF_Spindown_O.png');
}

.Calendar .calendarspindownbutton[userstatus=pushed],
.CalendarControl .calendarspindownbutton[userstatus=pushed]
{
	-nexa-icon 			: URL('theme://images/btn_WF_Spindown_P.png');
}

.Calendar .calendarspindownbutton[status=disabled],
.CalendarControl .calendarspindownbutton[status=disabled]
{
	-nexa-icon 			: URL('theme://images/btn_WF_Spindown_D.png');
}

/********************** CheckBox (CheckBoxControl)***********************************************/
.CheckBox,.CheckBoxControl
{
	-nexa-icon 			: URL('theme://images/chk_WF_Box.png');
	-nexa-text-padding 		: 0px 0px 2px 6px;
	color 				: #363636;
}

.CheckBox[status=mouseover],
.CheckBoxControl[status=mouseover]
{
	-nexa-icon 			: URL('theme://images/chk_WF_Box_O.png');
}

.CheckBox[status=readonly],
.CheckBoxControl[status=readonly]
{
	-nexa-icon 			: URL('theme://images/chk_WF_Box_D.png');
}

.CheckBox[userstatus=selected],
.CheckBoxControl[userstatus=selected]
{
	-nexa-icon 			: URL('theme://images/chk_WF_Box_S.png');
}

.CheckBox[status=disabled],
.CheckBoxControl[status=disabled]
{
	-nexa-icon 			: URL('theme://images/chk_WF_Box_D.png');
	color 				: #999999;
}

.CheckBox[status=disabled][userstatus=selected],
.CheckBoxControl[status=disabled][userstatus=selected]
{
	-nexa-icon 			: URL('theme://images/chk_WF_Box_DS.png');
	color 				: #999999;
}


/********************** Combo **********************/
.Combo,.ComboControl
{
	-nexa-border 			: 1px solid #c6c6c6;
	-nexa-padding 			: 0px 5px 1px 5px;
	font 				: 12px "Malgun Gothic";
	color 				: #363636;
	background 			: #ffffff;
}

.Combo[status=focused],.Combo[status=mouseover],.ComboControl[status=mouseover],.ComboControl[status=focused]
{
	-nexa-border 			: 1px solid #3da2df;
}

.Combo[status=readonly],.ComboControl[status=readonly]
{
	background 			: #efefef;
	color 				: #363636;
}

.Combo[status=disabled],.ComboControl[status=disabled]
{
	background 			: #efefef;
	color 				: #999999;
}

.Combo .comboedit,.ComboControl .comboedit
{
	-nexa-padding 			: 0px 5px 1px 2px;
	font 				: 12px "Malgun Gothic";
}

.Combo .dropbutton,.ComboControl .dropbutton,.Combo .dropbutton[status=focused]
{
	-nexa-padding 			: 1px 2px 1px 0px;
	-nexa-icon 			: URL('theme://images/cmb_WF_Drop.png');
}

.Combo .dropbutton[status=mouseover],.ComboControl .dropbutton[status=mouseover],.ComboControl .dropbutton[status=focused]
{
	-nexa-icon 			: URL('theme://images/cmb_WF_Drop_O.png');
}

.Combo .dropbutton[userstatus=pushed],.Combo .dropbutton[userstatus=selected],
.ComboControl .dropbutton[userstatus=pushed],.ComboControl .dropbutton[userstatus=selected]
{
	-nexa-icon 			: URL('theme://images/cmb_WF_Drop_P.png');
}

.Combo .dropbutton[status=disabled],.ComboControl .dropbutton[status=disabled]
{
	opacity 			: 0.50;
}

.Combo .combolist,.ComboControl .combolist
{
	font 				: 12px "Malgun Gothic";
	color 				: #363636;
	background 			: #ffffff;
	-nexa-border 			: 1px solid #c6c6c6;
}
.Combo .combolist[status=focused],.ComboControl .combolist[status=focused],
.Combo .combolist[status=mouseover],.ComboControl .combolist[status=mouseover]
{
	font 				: 12px "Malgun Gothic";
	color 				: #333333;
	background 			: #ffffff;
	-nexa-border 			: 1px solid #1fbbf4;
}


/********************** Edit **********************/
.Edit
{
	-nexa-border 			: 1px solid #c6c6c6;
	-nexa-padding 			: 0px 5px 0px 5px;
	background 			: #ffffff;
	color 				: #363636;
	font 				: 12px "Malgun Gothic";
}
.EditControl
{
	-nexa-border 			: 0px none;
	-nexa-padding 			: 0px 5px 0px 5px;
	background 			: transparent;
}
.Edit[status=focused],.Edit[status=mouseover]
{
	-nexa-border 			: 1px solid #3da2df;
	-nexa-padding 			: 0px 5px 0px 5px;
}
.Edit[status=readonly]
{
	background 			: #efefef;
}
.Edit[status=disabled]
{
	background 			: #efefef;
	color 				: #999999;
}

/********************** FileDownload **********************/
.FileDownload
{
	-nexa-border 			: 1px solid #c6c6c6;
	-nexa-padding 			: 0px 10px 2px 10px;
	background 			: #ffffff;
	color 				: #363636;
	font 				: bold 12px "Malgun Gothic";
}
.FileDownload[status=].FileDownload[status=mouseover]
{
	-nexa-border 			: 1px solid #a0a0a0;
	background 			: #fefefe;
}
.FileDownload[userstatus=pushed]
{
	background 			: #e9e9e9;
}
.FileDownload[status=disabled]
{
	-nexa-border 			: 1px solid #d9d9d9;
	background 			: #f5f5f5;
	color 				: #bbbbbb;
}

/********************** FileUpload **********************/
.FileUpload
{
	-nexa-border 			: 1px solid #c6c6c6;
	background 			: #ffffff;
	color 				: #363636;
	font 				: bold 12px "Malgun Gothic";
}
.FileUpload 
.fileuploaditem
{
	/*별도스타일없음*/
}
.FileUpload 
.fileuploaditem[status=disabled]
{
	color 				: #999999;
}

/********************** .FileUpload .fileuploaditem .fileuploaditembutt **********************/
.FileUpload 
.fileuploaditem 
.fileuploaditembutton
{
	-nexa-border 			: 0px none, 0px none, 0px none, 1px solid #c6c6c6;
	-nexa-padding 			: 2px 10px 3px 10px;
}
.FileUpload .fileuploaditem .fileuploaditembutton[status=mouseover],
.FileUpload .fileuploaditem .fileuploaditembutton[status=focused]
{
	
	-nexa-border 			: 0px none, 0px none, 0px none, 1px solid #c6c6c6;
	background 			: #fefefe;
}
.FileUpload .fileuploaditem .fileuploaditembutton[userstatus=pushed],
.FileUpload .fileuploaditem .fileuploaditembutton[userstatus=selected]
{
	-nexa-border 			: 0px none, 0px none, 0px none, 1px solid #c6c6c6;
	background 			: #e9e9e9;
}
.FileUpload .fileuploaditem .fileuploaditembutton[status=disabled]
{
	-nexa-border 			: 0px none, 0px none, 0px none, 1px solid #c6c6c6;
	background 			: #f5f5f5;
	color 				: #bbbbbb;
}

/********************** Grid **********************/
.Grid
{
	-nexa-border 			: 1px solid #4578aa, 1px solid #b7b1b2, 1px solid #b7b1b2, 1px solid #b7b1b2;
	background 			: #ffffff;
}

/*--------------------- head ---------------------*/
.Grid .head .row .cell
{
	-nexa-border 			: 1px solid #8f9eac, 1px solid #cacaca;
	-nexa-padding 			: 2px 2px 2px 2px;
	-nexa-text-align 		: center;
	background 			: #e1eaf3;
	color 				: #363636;
	font 				: bold 12px "Malgun Gothic";	
}
.Grid .head .row .cell[status=disabled]
{
	color 				: #999999;
}
.Grid .head .row .cell[status=disabled][userstatus=selected]
{
	background 			: #f0f0f0;
}
/*--------------------- headcellbutton:ButtonControl과 동일------------------------------------*/
/*--------------------- headcellcalendar:CalendarControl과 동일------------------------------------*/
/*--------------------- headcellcheckbox:CheckBoxControl과 동일------------------------------------*/
/*--------------------- headcellcombo:ComboControl과 동일------------------------------------*/
/*--------------------- headcelledit:EditControl과 동일------------------------------------*/
/*--------------------- headcellexpandbutton------------------------------------*/
/*--------------------- headcellimage:별도스타일없음------------------------------------*/
/*--------------------- headcellmaskedit:MaskEditControl과 동일------------------------------------*/
/*--------------------- headcellprogressbar:ProgressBarControl과 동일------------------------------------*/
/*--------------------- headcelltextarea:TextAreaControl과 동일------------------------------------*/
/*--------------------- headcelltreeitem:------------------------------------*/

/*--------------------- body------------------------------------*/
.Grid .body .row .cell
{
	-nexa-background-odd 		: #f9fafa;
	-nexa-border 			: 1px solid #dbdee2;
	-nexa-padding 			: 1px 1px 1px 1px;
	-nexa-text-align 		: center;
	color 				: #363636;
}
.Grid .body .row .cell[status=mouseover]
{
	background 			: #eef0d1;
	color 				: #363636;
}
.Grid .body .row .cell[userstatus=selected]
{
	/*background 			: #7cc6fe;*/
	background 			: #dde0ac;
	color 				: #363636;
}
.Grid .body .row .cell[status=disabled]
{
	color 				: #999999;
}

/*--------------------- bodycellbutton:ButtonControl과 동일------------------------------------*/
/*--------------------- bodycellcalendar:CalendarControl과 동일------------------------------------*/
/*--------------------- bodycellcheckbox:CheckBoxControl과 동일------------------------------------*/
/*--------------------- bodycellcombo:ComboControl과 동일------------------------------------*/
/*--------------------- bodycelledit:EditControl과 동일------------------------------------*/
/*--------------------- bodycellexpandbutton------------------------------------*/
/*--------------------- bodycellimage:별도스타일없음------------------------------------*/
/*--------------------- bodycellmaskedit:MaskEditControl과 동일------------------------------------*/
/*--------------------- bodycellprogressbar:ProgressBarControl과 동일------------------------------------*/
/*--------------------- bodycelltextarea:TextAreaControl과 동일------------------------------------*/
/*--------------------- bodycelltreeitem:------------------------------------*/

/*--------------------- summary------------------------------------*/
.Grid .summary
{
	-nexa-border 			: 1px solid #c8c1c2, 0px none, 0px none, 0px none;
}

.Grid .summary .row .cell
{
	-nexa-border 			: 1px solid #c8c1c2;
	-nexa-text-align 		: center;
	background 			: #fdfbe5;
	color 				: #363636;
	font 				: bold 12px "Malgun Gothic";
}

/*--------------------- summarycellbutton:ButtonControl과 동일------------------------------------*/
/*--------------------- summarycellcalendar:CalendarControl과 동일------------------------------------*/
/*--------------------- summarycellcheckbox:CheckBoxControl과 동일------------------------------------*/
/*--------------------- summarycellcombo:ComboControl과 동일------------------------------------*/
/*--------------------- summarycelledit:EditControl과 동일------------------------------------*/
/*--------------------- summarycellexpandbutton------------------------------------*/
/*--------------------- summarycellimage:별도스타일없음------------------------------------*/
/*--------------------- summarycellmaskedit:MaskEditControl과 동일------------------------------------*/
/*--------------------- summarycellprogressbar:ProgressBarControl과 동일------------------------------------*/
/*--------------------- summarycelltextarea:TextAreaControl과 동일------------------------------------*/
/*--------------------- summarycelltreeitem:------------------------------------*/

/*--------------------- Grid hscrollbar:------------------------------------*/
.Grid .hscrollbar
{
	-nexa-border 			: 1px solid #b1b1b1, 0px none, 0px none, 0px none;
}

/*--------------------- Grid vscrollbar:------------------------------------*/
.Grid .vscrollbar
{
	-nexa-border 			: 0px none, 0px none, 0px none, 1px solid #b1b1b1;
}


.GridCellControl .cellbutton[status=mouseover],
.GridCellControl .cellbutton[status=focused]
{
	-nexa-border 			: 1px solid #a0a0a0;
	background 			: #fefefe;
}
.GridCellControl .cellbutton[userstatus=pushed],
.GridCellControl .cellbutton[userstatus=selected]
{
	background 			: #e9e9e9;
}
.GridCellControl .cellbutton[status=disabled]
{
	-nexa-border 			: 1px solid #d9d9d9;
	background 			: #f5f5f5;
	color 				: #bbbbbb;
}

/*--------------------- cellcalendar:CalendarControl과 동일------------------------------------*/
.GridCellControl .cellcalendar
{
	-nexa-padding 			: 2px 2px 2px 2px;
	font 				: 12px "Malgun Gothic";
	color 				: #363636;
}

/*--------------------- cellcheckbox:CheckBoxControl과 동일------------------------------------*/
.GridCellControl .cellcheckbox
{
	-nexa-padding 			: 0px;
	font 				: 12px "Malgun Gothic";
}

/*--------------------- cellcombo:ComboControl과 동일------------------------------------*/
.GridCellControl .cellcombo
{
	font 				: 12px "Malgun Gothic";
	color 				: #363636;
}

.GridCellControl .cellcombo .combolist
{
	-nexa-padding 			: 2px 2px 2px 2px;
	font 				: 12px "Malgun Gothic";
	color 				: #363636;
}

.GridCellControl .cellcombo .combolist .listboxitem[status=mouseover]
{
	-nexa-padding 			: 2px 2px 2px 2px;
	background 			: #e8e9ed;
	color 				: #363636;
}

/*--------------------- celledit:EditControl과 동일------------------------------------*/
.GridCellControl .celledit,.GridCellControl .celledit[status=focused]
{
	-nexa-border 			: 1px solid #c6c6c6;
	-nexa-padding 			: 2px 2px 2px 2px;
	background 			: #ffffff;
	font 				: 12px "Malgun Gothic";
	color 				: #363636;
}
.GridCellControl .celledit[status=mouseover]
{
	-nexa-border 			: 1px solid #3da2df;
	color 				: #363636;
}
.GridCellControl .celledit[status=disabled]
{
	color 				: #999999;
	background 			: #efefef;
}

/*--------------------- cellexpandbutton------------------------------------*/

.GridCellControl .cellexpandbutton
{
	-nexa-icon 			: URL('theme://images/btn_WF_Grdexpand.png');
}

/*--------------------- cellimage:별도스타일없음------------------------------------*/

/*--------------------- cellmaskedit:MaskEditControl과 동일------------------------------------*/
.GridCellControl .cellmaskedit,.GridCellControl .cellmaskedit[status=focused]
{
	-nexa-border 			: 1px solid #c6c6c6;
	-nexa-padding 			: 2px 2px 2px 2px;
	background 			: #ffffff;
	font 				: 12px "Malgun Gothic";
	color 				: #363636;
}
.GridCellControl .cellmaskedit[status=mouseover]
{
	-nexa-border 			: 1px solid #3da2df;
	color 				: #363636;
}
.GridCellControl .cellmaskedit[status=disabled]
{
	color 				: #999999;
	background 			: #efefef;
}

/*--------------------- cellprogressbar:ProgressBarControl과 동일------------------------------------*/
.GridCellControl .cellprogressbar
{
	font 				: 12px "Malgun Gothic";
}

/*--------------------- celltextarea:TextAreaControl과 동일------------------------------------*/
.GridCellControl .celltextarea
{
	font 				: 12px "Malgun Gothic";
	color 				: #363636;
}

/*--------------------- celltreeitem:------------------------------------*/
.GridCellControl .celltreeitem .treeitemtext
{
	font 				: 12px "Malgun Gothic";
	color 				: #363636;
}

.GridCellControl .celltreeitem .treeitemtext[status=disabled]
{
	color 				: #999999;
	color 				: #363636;
}

.GridCellControl .celltreeitem .treeitembutton[userstatus=expand]
{
	-nexa-icon 			: URL('theme://images/btn_WF_Treeexpand.png');
}

.GridCellControl .celltreeitem .treeitembutton[userstatus=collapse]
{
	-nexa-icon 			: URL('theme://images/btn_WF_Treecollapse.png');
}

.GridCellControl .celltreeitem .treeitemimage
{
	-nexa-icon 			: URL('theme://images/img_WF_Treeitem.png');
}

.GridCellControl .celltreeitem .treeitemimage[userstatus=expand]
{
	-nexa-icon 			: URL('theme://images/img_WF_Treeexpand.png');
}

.GridCellControl .celltreeitem .treeitemimage[userstatus=collapse]
{
	-nexa-icon 			: URL('theme://images/img_WF_Treecollapse.png');
}

.GridCellControl .celltreeline
{
	-nexa-border 			: 1px dotted #909090;
}



/********************** GroupBox***********************************************/
.GroupBox
{
	/*별도스타일없음*/
}

.GroupBox .groupboxcontents
{
	background 			: #ffffff;
	-nexa-border 			: 1px solid #d6d4d5;
}

.GroupBox .groupboxcontents[status=disabled]
{
	background 			: #efefef;
}

.GroupBox .groupboxtitle
{
	background 			: #ffffff;
	-nexa-padding 			: 0px 4px 0px 4px;
}

.GroupBox .groupboxtitle[status=disabled]
{
	color 				: #999999;
	background 			: #efefef;
}


/********************** ImageViewer***********************************************/
.ImageViewer
{
	background 			: #ffffff;
	-nexa-border 			: 1px solid #c6c6c6;
}

.ImageViewer[status=disabled]
{
	background 			: #f2f2f2;
}

.ImageViewer .imagetext
{
	-nexa-text-align 		: center;
}

.ImageViewer .imagetext[status=disabled]
{
	color 				: #999999;
}


/********************** ListBox***********************************************/
.ListBox,.ListBoxControl
{
	-nexa-border 			: 1px solid #c6c6c6;
	background 			: #ffffff;
}

.ListBox[status=focused],.ListBoxControl[status=focused],
.ListBox[status=mouseover],.ListBoxControl[status=mouseover]
{
	-nexa-border 			: 1px solid #3da2df;
}
.ListBox[status=disabled],.ListBoxControl[status=disabled]
{
	background 			: #efefef;
}

.ListBox .listboxitem,.ListBoxControl .listboxitem
{
	-nexa-padding 			: 5px 7px 6px 7px;
	color 				: #363636;
}
.ListBox .listboxitem[status=mouseover],.ListBoxControl .listboxitem[status=mouseover]
{
	background 			: #e7e9ed;
}
.ListBox .listboxitem[userstatus=selected],.ListBoxControl .listboxitem[userstatus=selected]
{
	color 				: #ffffff;
	background 			: #125dae;
}
.ListBox .listboxitem[status=disabled],.ListBoxControl .listboxitem[status=disabled]
{
	/*color 				: #999999;*/
	opacity 			: 0.50;
}


/********************** MaskEdit***********************************************/
.MaskEdit
{
	-nexa-border 			: 1px solid #c6c6c6;
	-nexa-padding 			: 0px 5px 0px 5px;
	background 			: #ffffff;
	color 				: #363636;
}
.MaskEditControl
{
	background 			: transparent;
	-nexa-border 			: 0px none;
	-nexa-padding 			: 0px 5px 0px 5px;
}
.MaskEdit[status=focused],.MaskEdit[status=mouseover]
{
	-nexa-border 			: 1px solid #3da2df;
}
.MaskEdit[status=readonly]
{
	background 			: #efefef;
	color 				: #363636;
}
.MaskEdit[status=disabled]
{
	background 			: #f2f2f2;
	color 				: #999999;
}


/********************** Menu***********************************************/
.Menu
{
	background 			: transparent;
}
.Menu[status=disabled]
{
	background 			: transparent;
}
