	{
		var oArg = {gongganCdNm:strSearchValue};
		var oOption = {};
		var sPopupCallBack = "fn_popupCallback";
		this.gfn_openPopup( "gonggan","com::COMM_P08.xfdl",oArg,sPopupCallBack,oOption);
	}
}


