//중복체크
this.fn_Unique = function (params)
{
    var uniqueCk = 0;
    var array = new Array();
    for(var i = 0 ; i < params.length; i++){
        for(var j = 0 ; j < array.length ; j++){
            if(array[j] == params[i]){
                uniqueCk = 1;
                break;
            }
        }
        array[array.length] = params[i];
    }
    return uniqueCk;
};

