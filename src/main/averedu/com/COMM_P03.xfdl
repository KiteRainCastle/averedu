	{
		//13(Enter) 누르면 조회
		if(e.keycode == 13)		
		{
			this.ds_input.setColumn(0,"GYOGWAMOK_CD_NM",obj.value);
			this.fn_Ret();
		}
	}
};
]]></Script>
